// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Post {
  id      Int     @id @default(autoincrement())
  title   String  @db.VarChar(255)
  details String?
  images  Json
  years   Int
  link    String
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?

}
